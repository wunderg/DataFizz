{
  "name": "illbethereforyou",
  "version": "1.0.0",
  "description": "Social Network",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "NODE_ENV=development babel-node ./server/server.js",
    "start-prod": "node ./dist/server.js",
    "build-client": "NODE_ENV=production webpack --config ./webpack.production.config.js --profile",
    "build-server": "babel -d ./dist ./server -s",
    "lint": "eslint src; exit 0",
    "clean": "rimraf dist",
    "test": "mocha --compilers js:babel-core/register --require ignore-styles --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "heroku-postbuild": "npm run clean && npm run build-client && npm run build-server"
  },
  "author": "Oleg Umarov",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "body-parser": "^1.15.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-react": "^5.1.1",
    "eslint_d": "^3.1.1",
    "ignore-styles": "^2.0.0",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0"
  }
}
